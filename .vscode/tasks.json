{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "tmbuild",
            "type": "process",
            "command": "tmbuild.exe",
            "args": ["--no-unit-test"],
            "options": {
                "cwd": "${workspaceFolder}/plugins"
            },
            "presentation": {
                "echo": true,
                "reveal": "silent",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": true
            },
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
